    "@nestjs/testing": "^11.1.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cache-manager-redis-store": "^3.0.0",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/joi": "^17.2.3",
    "@types/mime-types": "^2.1.4",
